{
    "name": "rnr1721/le7-cache",
    "description": "PSR-16 SimpleCache for Le7 framework or any other PHP projects",
    "keywords": ["le7", "le7-framework", "framework", "cache", "component"],
    "authors": [
        {
            "name": "Eugeny G",
            "email": "rnr1721@gmail.com",
            "homepage": "https://xhamster.biz.ua",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Core\\Cache\\": "src"
        }
    },
    "authors": [
        {
            "name": "rnr1721",
            "email": "rnr1721@gmail.com"
        }
    ],
    "provide": {
        "psr/simple-cache-implementation": "3.0.0"
    },
    "require": {
        "php": ">=8.1",
        "psr/simple-cache": "^3.0"
    },
    "require-dev": {
        "vimeo/psalm": "^5.7",
        "phpunit/phpunit": "^10.0"
    },
    "suggest": {
        "rnr1721/le7-cache-memcache": "Needed to support Memcache and memcached"
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@psalm"
        ],
        "phpunit": "phpunit",
        "psalm": "psalm"
    }
}
